{
  "title": "条形图",
  "namespace": "mybricks.normal-pc-chart.bar",
  "version": "1.0.8",
  "description": "条形图",
  "author": "MyBricks",
  "author_name": "MyBricks",
  "icon": "./icon.svg",
  "data": "./data.json",
  "runtime": "./runtime.tsx",
  "editors": "./editor.tsx",
  "upgrade": "./upgrade.ts",
  "inputs": [
    {
      "id": "data",
      "title": "输入数据",
      "schema": {
        "title": "输入数据",
        "type": "array",
        "items": {
          "type": "object",
          "properties":{
            "xField": {
              "title": "x轴字段名",
              "type": "string"
            },
            "yField": {
              "title": "y轴字段名",
              "type": "number"
            },
            "seriesField": {
              "title": "分组字段名",
              "type": "string"
            }
          }
        }
      }
    }
  ],
  "outputs": [
    {
      "title": "元素点击",
      "id": "eleClick",
      "schema": {
        "type": "object",
        "properties": {
          "data": {
            "type": "object"
          },
          "element": {
            "type": "object",
            "properties": {
              "states": {
                "type": "array",
                "items": {
                  "type": "enum",
                  "items": [
                    {
                      "type": "string",
                      "value": "active"
                    },
                    {
                      "type": "string",
                      "value": "selected"
                    },
                    {
                      "type": "string",
                      "value": "inactive"
                    }
                  ]
                }
              }
            }
          }
        }
      }
    }
  ]
}
